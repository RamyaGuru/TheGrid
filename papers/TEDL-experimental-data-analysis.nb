        RowBox[{"(", 
         RowBox[{"F", "[", 
          RowBox[{
           RowBox[{"1.5", "+", "\[Lambda]"}], ",", "\[Eta]"}], "]"}], ")"}], 
        "2"]}]}], 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"1.5", "+", "\[Lambda]"}], ")"}], "2"], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"F", "[", 
          RowBox[{
           RowBox[{"0.5", "+", "\[Lambda]"}], ",", "\[Eta]"}], "]"}], ")"}], 
        "2"]}], ")"}]]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.64710580964834*^9, 3.6471058687533712`*^9}}],

