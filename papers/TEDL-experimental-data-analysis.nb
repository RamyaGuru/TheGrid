Cell[CellGroupData[{

Cell["Chemical Potential from Seebeck Coefficient", "Subsection",
 CellChangeTimes->{{3.647105652771858*^9, 3.6471056616344013`*^9}}],

Cell[TextData[{
 StyleBox["First, we define the ",
  FontSize->16],
 StyleBox["Fermi-Dirac Distribution (P)",
  FontSize->16,
  FontWeight->"Bold"],
 StyleBox[" and the associated ",
  FontSize->16],
 StyleBox["Fermi integral (F)",
  FontSize->16,
  FontWeight->"Bold"],
 StyleBox[" in terms of the ",
  FontSize->16],
 StyleBox["energy (\[CapitalEpsilon])",
  FontSize->16,
  FontWeight->"Bold"],
 StyleBox[" and the ",
  FontSize->16],
 StyleBox["chemical potential (\[Eta])",
  FontSize->16,
  FontWeight->"Bold"],
 StyleBox[". In this context, the ",
  FontSize->16],
 StyleBox["scattering factor (r)",
  FontSize->16,
  FontWeight->"Bold"],
 StyleBox[" is related to \[Lambda] (from RTA) through the Fermi function. ",
  FontSize->16]
}], "Text",
 CellChangeTimes->{{3.64709102106175*^9, 3.647091021948144*^9}, {
  3.647091080693645*^9, 3.64709118404953*^9}, {3.647093255238982*^9, 
  3.647093288901333*^9}, {3.6470933278543787`*^9, 3.647093394900157*^9}, {
  3.647093471619975*^9, 3.647093504946855*^9}, {3.6471003060801983`*^9, 
  3.647100329853668*^9}, {3.647100367916483*^9, 3.647100480855365*^9}, {
  3.6471011538730164`*^9, 3.647101164511643*^9}, {3.647105618137521*^9, 
  3.647105643939826*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"P", "[", 
    RowBox[{"\[Epsilon]_", ",", "r_", ",", "\[Eta]_"}], "]"}], ":=", 
   FractionBox[
    SuperscriptBox["\[Epsilon]", "r"], 
    RowBox[{"1", "+", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{"\[Epsilon]", "-", "\[Eta]"}]]}]]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"F", "[", 
    RowBox[{"r_", ",", "\[Eta]_"}], "]"}], ":=", 
   RowBox[{
    SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
    RowBox[{
     FractionBox[
      SuperscriptBox["\[Epsilon]", "r"], 
      RowBox[{"1", "+", 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"\[Epsilon]", "-", "\[Eta]"}]]}]], 
     RowBox[{"\[DifferentialD]", "\[Epsilon]"}]}]}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.647093396548781*^9, 3.647093468772438*^9}, {
  3.647100177293262*^9, 3.647100204696081*^9}, {3.647100356979928*^9, 
  3.647100357612823*^9}, {3.647100496635044*^9, 3.647100512449856*^9}, {
  3.64710056961693*^9, 3.647100613960528*^9}, {3.647101168368829*^9, 
  3.6471011721788187`*^9}, {3.6471070555958977`*^9, 3.647107087726245*^9}}],

Cell[TextData[{
 StyleBox["Recall that the ",
  FontSize->16],
 StyleBox["Seebeck Coefficient (\[Alpha]) ",
  FontSize->16,
  FontWeight->"Bold"],
 StyleBox["can be defined in terms of the chemical potential (\[Eta]), the \
Fermi integral (F), and the ",
  FontSize->16],
 StyleBox["RTA scattering factor (\[Lambda])",
  FontSize->16,
  FontWeight->"Bold"],
 StyleBox[":",
  FontSize->16]
}], "Text",
