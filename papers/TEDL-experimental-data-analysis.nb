        RowBox[{"3.5", "+", "\[Lambda]"}], ")"}], 
       RowBox[{"F", "[", 
        RowBox[{
         RowBox[{"0.5", "+", "\[Lambda]"}], ",", "\[Eta]"}], "]"}], 
       RowBox[{"F", "[", 
        RowBox[{
         RowBox[{"2.5", "+", "\[Lambda]"}], ",", "\[Eta]"}], "]"}]}], "-", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
