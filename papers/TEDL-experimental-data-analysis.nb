Cell[TextData[{
 StyleBox["Recall that the ",
  FontSize->16],
 StyleBox["Lorentz number (L) ",
  FontSize->16,
  FontWeight->"Bold"],
 StyleBox["is defined in terms of the ",
  FontSize->16],
 StyleBox["chemical potential (\[Eta])",
  FontSize->16,
  FontWeight->"Bold"],
 StyleBox[" and the ",
  FontSize->16],
 StyleBox["Fermi integral (F)",
  FontSize->16,
  FontWeight->"Bold"],
 StyleBox[" as",
  FontSize->16]
}], "Text",
 CellChangeTimes->{{3.64709102106175*^9, 3.647091021948144*^9}, {
  3.647091080693645*^9, 3.64709118404953*^9}, {3.647093255238982*^9, 
  3.647093288901333*^9}, {3.6470933278543787`*^9, 3.647093394900157*^9}, {
  3.647093471619975*^9, 3.647093504946855*^9}, {3.6471003060801983`*^9, 
  3.647100329853668*^9}, {3.647100367916483*^9, 3.647100480855365*^9}, {
  3.647100639826672*^9, 3.647100664138228*^9}, {3.647100873681419*^9, 
  3.6471010368269444`*^9}, {3.647101216374881*^9, 3.6471012399809437`*^9}, {
  3.647101274348948*^9, 3.647101301778636*^9}, {3.647101687021575*^9, 
  3.647101700396119*^9}, {3.647102538380013*^9, 3.647102588417457*^9}, {
  3.647105600773965*^9, 3.647105605044519*^9}, {3.6471057111083393`*^9, 
  3.647105720228109*^9}, {3.647105876296144*^9, 3.647105911238614*^9}, {
  3.647606759680676*^9, 3.647606759749131*^9}, {3.647606808803192*^9, 
  3.647606809162779*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"L", "[", 
    RowBox[{"\[Lambda]_", ",", "\[Eta]_"}], "]"}], ":=", " ", 
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      FractionBox["k", "q"], ")"}], "2"], 
    FractionBox[
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1.5", "+", "\[Lambda]"}], ")"}], 
